<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="SCSS" injector-id="js">
      <display-name>styled-components</display-name>
      <prefix>dummy_selector {</prefix>
      <suffix>}</suffix>
      <place><![CDATA[jsLiteralExpression().withText(string().matchesBrics("styled.+"))]]></place>
    </injection>
    <injection language="CSS" injector-id="xml">
      <display-name>Modal/@bodyStyle</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("bodyStyle")).withParent(xmlTag().withLocalName(string().equalTo("Modal")))]]></place>
    </injection>
  </component>
</application>